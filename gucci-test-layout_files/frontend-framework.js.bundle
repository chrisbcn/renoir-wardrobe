                if (!insideFrontInterface.chat.chatPaneOpen && $("#inside_tabs").is(":visible") && !$("#inside_tabs").is(':empty') && $("#inside_liveChatTab").height() == 0 && $(".inside-notification").length == 0) {
                    setTimeout(windowScale, 250);
