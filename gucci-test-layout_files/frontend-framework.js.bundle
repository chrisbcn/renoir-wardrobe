            }

            if(tabPosition.startsWith('top')) {
                offset -= $("#inside_liveChatTab").height();
                if($('#inside_clickToCallChatTab').height()) {
                    offset -= $('#inside_clickToCallChatTab').height();
                }
            }

            if (!animTime)  {
                if (!(ios && !standalone && safari && $('input').is(":focus"))) {
